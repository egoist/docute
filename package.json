    "vuex-router-sync": "^4.1.2"
  },
  "devDependencies": {
    "cssbag": "^0.1.0",
    "cssnano": "^3.10.0",
    "hint.css": "^2.4.1",
    "jest-cli": "^19.0.2",
    "opencollective-postinstall": "^1.0.13",
    "postcss": "^5.2.16",
    "raw-loader": "^0.5.1",
    "vbuild": "^6.24.7"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "collective": {
