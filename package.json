  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "npm run build",
    "build": "vbuild -c build/vue.config.js --eslint && node build/themes",
    "dev": "vbuild -d --config build/vue.config.js",
    "dev:docsearch": "vbuild -d --config build/vue.config.docsearch.js",
    "build:docsearch": "vbuild --config build/vue.config.docsearch.js",
    "dev:disqus": "vbuild -d --config build/vue.config.disqus.js",
    "build:disqus": "vbuild --config build/vue.config.disqus.js",
    "prepublish": "npm run build",
    "postinstall": "./node_modules/.bin/opencollective-postinstall || exit 0"
  },
  "files": [
    "dist/docute.js",
    "dist/docute.css",
    "dist/docute.js.map",
    "dist/docute.css.map",
    "dist/theme-github.css",
    "dist/theme-github.css.map",
    "plugins"
  ],
  "main": "./dist/docute.js",
  "style": "./dist/docute.css",
  "keywords": [
    "doc"
  ],
  "dependencies": {
    "algoliasearch": "^3.21.1",
    "babel-runtime": "^6.20.0",
    "defined": "^1.0.0",
    "dom-scroll-into-view": "^1.2.1",
    "element-closest": "^2.0.2",
    "eventstop": "^2.1.0",
    "jump.js": "^1.0.2",
    "lodash.throttle": "^4.1.1",
    "nprogress": "^0.2.0",
    "prismjs": "^1.6.0",
    "promise-polyfill": "^6.0.2",
    "unfetch": "^2.1.2",
    "url-resolve": "^1.0.0",
